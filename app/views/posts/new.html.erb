<section>
  <h1>Create a New Post</h1>
  <%= form_with model: Post, url: user_posts_url do |form|%>
    <div>
      <%= form.label :title%>
      <%= form.text_field :title%>
    </div>
    <div>
      <%= form.label :text%>
      <%= form.text_field :text%>
    </div>
    <div>
      <%= form.submit "Create Post" %><br>
    </div>
  <%end%>
</section>
